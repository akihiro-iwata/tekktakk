{
  "name": "tekktakk",
  "version": "1.0.0",
  "description": "share your techtalk",
  "author": "akihiro-iwata@m3",
  "private": true,
  "scripts": {
    "dev": "export ENV=dev && export API_URL=http://localhost:4000 && nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "deploy": "firebase use tekktakk-dev && export ENV=stg && export API_URL=https://tekktakk-dev.herokuapp.com && nuxt generate && firebase deploy --only hosting:tekktakk-dev",
    "prod-deploy": "firebase use tekktakk-c214a && export ENV=prod && export API_URL=https://tekktakk.herokuapp.com && nuxt generate && firebase deploy --only hosting:tekktakk-c214a"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-0",
    "dotenv": "^8.1.0",
    "firebase": "^7.0.0",
    "nuxt": "^2.0.0",
    "video.js": "^7.6.5",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-jest": "^4.0.0-0"
  }
}
